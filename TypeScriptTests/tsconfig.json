{
	"compileOnSave": true,
	"compilerOptions": {
		"noImplicitAny": false,
		"strictNullChecks": true,
		"noEmitOnError": true,
		"removeComments": false,
		"sourceMap": true,
		"target": "es5",
		"outDir": "wwwroot/js"
	},
	"include": [
		"wwwroot/ts/**/*.ts"
	],
	"exclude": [
		"**/node_modules"
	]
}